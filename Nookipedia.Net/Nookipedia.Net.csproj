<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <Version>1.3.0.2</Version>
    <Authors>Micalobia</Authors>
    <PackageId>Micalobia_Nookipedia.Net</PackageId>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Copyright></Copyright>
    <PackageLicenseUrl>https://choosealicense.com/licenses/gpl-3.0/</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Micalobia/Nookipedia.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Micalobia/Nookipedia.Net</RepositoryUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>A C# wrapper for the API provided by Nookipedia for Animal Crossing</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Macross.Json.Extensions" Version="2.0.0" />
    <PackageReference Include="System.Text.Json" Version="5.0.1" />
  </ItemGroup>

</Project>
